# include<iostream>
using namespace std;

int main()
{
    int a,b;
    float x,y,div,mod,multiply;
    double i,j,add,sub;
    scanf("%d %d %f %f %lf %lf",&a,&b,&x,&y,&i,&j);
    if(x>y)
    {
    div=x/y;
    mod=x%y;
    }
    else
    {
        div=y/x;
        mod=y%x;
    }
    multiply=x*y;
    add=i+j;
    sub=i-j;
    a=++a+b;
    b=b-- +a;
    printf("%lf %lf %f %f %f %d %d",add,sub,multiply,mod,div,a,b);
 return 0;
}
